import requests

headers = {'User-Agent': "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/87.0.4280.88 Safari/537.36"} # This is chrome, you can set whatever browser you like
session = requests.Session()

url = "http://89f10ec78c75.ngrok.io"
url = url +'/topi'

myobj = "Temp: 100F, date: 7 JAN 2021"
r = session.post(url, headers=headers, json=myobj)
print(r.status_code)
print(r.encoding)
print(r.apparent_encoding)
print(r.text)
